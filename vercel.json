{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "webapp/backend/src/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "webapp/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/webapp/backend/src/index.ts"
    },
    {
      "src": "/debug/(.*)",
      "dest": "/webapp/backend/src/index.ts"
    },
    {
      "src": "/health",
      "dest": "/webapp/backend/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/webapp/frontend/$1"
    }
  ],
  "env": {
    "DATABASE_TYPE": "postgresql",
    "ENVIRONMENT": "production"
  }
}