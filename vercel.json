{
  "version": 2,
  "framework": null,
  "functions": {
    "webapp/backend/dist/index.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/webapp/backend/dist/index.js"
    },
    {
      "source": "/debug/:path*",
      "destination": "/webapp/backend/dist/index.js"
    },
    {
      "source": "/health",
      "destination": "/webapp/backend/dist/index.js"
    }
  ],
  "env": {
    "DATABASE_TYPE": "postgresql",
    "ENVIRONMENT": "production"
  },
  "buildCommand": "cd webapp && npm install && npm run build",
  "outputDirectory": "webapp/frontend/.next",
  "installCommand": "cd webapp && npm install"
}